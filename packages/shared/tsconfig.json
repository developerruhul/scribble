{
  "compilerOptions": {
    "jsx": "react",
    "target": "ES2020",
    "module": "commonjs",
    "resolveJsonModule": true,
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "rootDir": ".",
    "outDir": "dist"
  }
}
